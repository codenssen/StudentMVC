<!DOCTYPE html>
<html>
<head>
    <title>
        MVC TEST
    </title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
    <nav class="bg-white border-gray-200 dark:bg-gray-900">
        <div class="p-4 mx-auto flex max-w-screen-xl flex-wrap items-center justify-between">

            <span class="self-center whitespace-nowrap text-2xl font-semibold dark:text-white">Application MVC test</span>

            <button data-collapse-toggle="navbar-default" type="button" class="p-2 w-10 h-10 text-gray-500 inline-flex items-center justify-center rounded-lg text-sm hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 md:hidden dark:hover:bg-gray-700 dark:focus:ring-gray-600" aria-controls="navbar-default" aria-expanded="false">
                <span class="sr-only">Open main menu</span>
                <svg class="w-5 h-5" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 17 14">
                    <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 1h15M1 7h15M1 13h15" />
                </svg>
            </button>
            <div class="hidden w-full md:block md:w-auto" id="navbar-default">
                <ul class="p-4 mt-4 border-gray-100 bg-gray-50 flex flex-col rounded-lg border font-medium rtl:space-x-reverse dark:bg-gray-800 dark:border-gray-700 md:p-0 md:flex-row md:space-x-8 md:mt-0 md:border-0 md:bg-white md:dark:bg-gray-900">
                    <li>
                        <a class="py-2 px-3 text-white bg-blue-700 block rounded dark:text-white md:bg-transparent md:text-blue-700 md:p-0 md:dark:text-blue-500" aria-current="page"
                           asp-controller="Student"
                           asp-action="List">
                            Etudiant
                        </a>
                    </li>
                    <li>
                        <a class="py-2 px-3 text-white bg-blue-700 block rounded dark:text-white md:bg-transparent md:text-blue-700 md:p-0 md:dark:text-blue-500" aria-current="page"
                           asp-controller="Course"
                           asp-action="List">
                            Course
                        </a>
                    </li>


                </ul>
            </div>
        </div>
    </nav>

    <main role="main" class="p-20">
        @RenderBody()
    </main>
</body>
</html>